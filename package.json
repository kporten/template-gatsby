{
  "name": "template-gatsby",
  "version": "1.0.6",
  "private": false,
  "license": "Unlicense",
  "description": "Template for a website with Gatsby and TypeScript",
  "repository": "https://github.com/kporten/template-gatsby.git",
  "author": "Kevin Porten <hello@kevinporten.dev>",
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx,.js,.jsx .",
    "hint": "hint ./src",
    "test": "jest --watch",
    "test:ci": "jest --passWithNoTests",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:ci": "start-server-and-test start 8000 e2e:run",
    "tsc:check": "tsc --project ./tsconfig.json && tsc --project ./cypress/tsconfig.json",
    "version": "auto-changelog --package --template keepachangelog --sort-commits relevance && git add CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "commit": "git-cz"
  },
  "dependencies": {
    "gatsby": "^2.23.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/dom": "^7.11.0",
    "@testing-library/jest-dom": "^5.10.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^11.4.1",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "auto-changelog": "^2.0.0",
    "babel-jest": "^26.0.1",
    "babel-preset-gatsby": "^0.4.9",
    "commitizen": "^4.1.2",
    "cypress": "^4.8.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jest-dom": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-testing-library": "^3.2.2",
    "eslint-plugin-unicorn": "^20.1.0",
    "hint": "^6.0.6",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "start-server-and-test": "^1.11.0",
    "typescript": "^3.9.5"
  }
}
