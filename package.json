{
  "name": "template-gatsby",
  "version": "1.2.0",
  "private": false,
  "license": "MIT",
  "description": "Template for a website with Gatsby and TypeScript",
  "repository": "https://github.com/kporten/template-gatsby.git",
  "author": "Kevin Porten <hello@kevinporten.dev>",
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx,.js,.jsx .",
    "hint": "hint ./src",
    "type-check": "tsc --project ./tsconfig.json && tsc --project ./cypress/tsconfig.json",
    "changelog": "auto-changelog --package --template keepachangelog --sort-commits relevance",
    "test": "jest --watchAll --coverage --notify",
    "test:ci": "jest --passWithNoTests",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:ci": "start-server-and-test start 8000 e2e:run",
    "commit": "git-cz",
    "version": "yarn changelog && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "gatsby": "^2.24.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/dom": "^7.21.5",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^12.0.15",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "auto-changelog": "^2.2.0",
    "babel-jest": "^26.1.0",
    "babel-preset-gatsby": "^0.5.4",
    "commitizen": "^4.1.2",
    "cypress": "^4.11.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.2",
    "eslint-plugin-jest-dom": "^3.1.4",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^3.3.2",
    "eslint-plugin-unicorn": "^21.0.0",
    "hint": "^6.0.6",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.2",
    "typescript": "^3.9.7"
  }
}
